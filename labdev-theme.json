  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6A9955",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ff00ff"
      }
    },
    {
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#ff00ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#ff00ff"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#ff00ff"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class"],
      "settings": {
        "foreground": "#4ec9b0"
      }
    },
    {
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#4ec9b0"
      }
    },
    {
      "scope": ["support.function"],
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "scope": ["meta.brace"],
      "settings": {
        "foreground": "#ff00ff"
      }
    }
  ]
